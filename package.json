{
  "name": "get-tweets",
  "version": "0.1.2",
  "description": "Get all tweets for target username",
  "main": "index.es5.js",
  "scripts": {
    "coverage": "istanbul cover _mocha -- --timeout 300000 --require babel/register",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test": "mocha --timeout 300000 --require babel/register",
    "tdd": "npm test -- --watch",
    "transpile": "babel index.js > index.es5.js",
    "prepublish": "npm run transpile",
    "postpublish": "rm *.es5.js && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamstarkov/get-tweets.git"
  },
  "keywords": [
    "twitter",
    "data",
    "automatisation"
  ],
  "author": "Vladimir Starkov <iamstarkov@gmail.com> (http://iamstarkov.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamstarkov/get-tweets/issues"
  },
  "homepage": "https://github.com/iamstarkov/get-tweets#readme",
  "dependencies": {
    "bignum": "^0.10.0",
    "bluebird": "^2.9.24",
    "twitter": "^1.2.5"
  },
  "devDependencies": {
    "babel": "^5.1.13",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.14",
    "mocha": "^2.2.4"
  }
}
